{
    "name": "getsky/phalcon-autoload-services",
    "type": "library",
    "description": "Components for automatic registration services in Dependency Injection",
    "keywords": [ "framework", "phalcon", "di", "dependency injection" ],
    "license": "MIT",
    "authors": [
        {
            "name": "JimmDiGrizli",
            "email": "getmansk-y@yandex.ru"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-phalcon": ">=1.2.4,<2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "squizlabs/php_codesniffer": "1.*",
        "codeception/codeception": "*"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "1.1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "GetSky\\Phalcon\\AutoloadServices\\": "src/"
        }
    }
}
